{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.Hosting.Lifetime": "Information",
            "CarTelemetryService": "Debug"
        }
    },
    "Kafka": {
        "BootstrapServers": "localhost:9092",
        "GroupId": "car-telemetry-consumer-group",
        "Topics": [
            "orgcarfleet-car-events"
        ],
        "AutoOffsetReset": "latest",
        "EnableAutoCommit": false
    },
    "Postgres": {
        "Host": "localhost",
        "Port": 5432,
        "Database": "orgcarfleet",
        "Username": "postgres",
        "Password": "postgres"
    }
}